{
  "name": "icon",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes",
    "l-changed": "lerna changed",
    "l-publish": "lerna publish",
    "l-publish-package": "lerna publish from-package",
    "dev": "dumi dev",
    "doc:github-build": "cross-env PUBLIC_PATH=icon-doc/ dumi build",
    "build:web": "cd packages/icons-react && father build",
    "build:app": "cd packages/icons-react-native && father build",
    "build:taro": "cd packages/icons-taro && father build",
    "ncu": "npx npm-check-updates -u",
    "gen:iconfont:web": "ts-node --project tsconfig.json libs/iconfont-parser.ts type=web",
    "gen:iconfont:app": "ts-node --project tsconfig.json libs/iconfont-parser.ts type=app",
    "gen:iconpark:web": "ts-node --project tsconfig.json libs/iconpark-parser.ts type=web",
    "gen:iconpark:app": "ts-node --project tsconfig.json libs/iconpark-parser.ts type=app",
    "gen:iconpark:taro": "ts-node --project tsconfig.json libs/iconpark-parser.ts type=taro",
    "commit": "npx git-cz",
    "lint:ts": "tsc --noEmit && eslint packages/**/src --ext .ts,.tsx"
  },
  "devDependencies": {
    "@fruits-chain/eslint-config-preset": "^0.2.8",
    "@fruits-chain/prettier-config-preset": "^0.0.3",
    "@tarojs/components": "^3.6.8",
    "@types/lodash": "^4.14.189",
    "@types/node": "^18.11.9",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.25",
    "@types/react-native": "^0.70.6",
    "@types/xml2js": "^0.4.11",
    "antd": "^4.24.2",
    "babel-plugin-import": "^1.13.5",
    "copy-to-clipboard": "^3.3.3",
    "core-js": "^3.26.1",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.49",
    "father": "^4.1.0",
    "lerna": "^6.0.3",
    "node-fetch": "^3.3.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-native-svg": "^13.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "umi-plugin-react-native": "^0.5.0",
    "xml2js": "^0.4.23"
  },
  "prettier": "@fruits-chain/prettier-config-preset",
  "resolutions": {
    "prettier": "^2.5.1"
  }
}
